{
  "expo": {
    "name": "Wellly",
    "slug": "wellly",
    "version": "2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash_wellly3.png",
      "resizeMode": "contain",
      "backgroundColor": "#59CBE4"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.planforfit.wellly",
      "buildNumber": "2",
      "infoPlist": {
        "NSMotionUsageDescription": "This app uses motion data to track your steps and activity.",
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to track your activity.",
        "NSLocationAlwaysUsageDescription": "This app needs access to your location to track your activity."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "versionCode": 20,
      "package": "com.planforfit.wellly",
      "permissions": [
        "ACTIVITY_RECOGNITION",
        "INTERNET",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "120b195b-b1c0-44f9-a75e-1794dd3c2746"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 33,
            "targetSdkVersion": 33
          }
        }
      ]
    ]
  }
}
